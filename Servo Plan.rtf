  -e POSTGRES_PASSWORD=servo \\\
  -e POSTGRES_USER=servo \\\
  -e POSTGRES_DB=servo_metadata \\\
  -p 5432:5432 \\\
  postgres:15\
\
# Run migrations\
cargo run --bin servo-cli -- migrate up\
\
# Run tests\
cargo test\
pytest\
\

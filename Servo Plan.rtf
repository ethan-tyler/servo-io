\f0\b \cf0 Storage:
\f1\b0  Lineage stored as directed acyclic graph (DAG) in PostgreSQL:\
\pard\pardeftab720\partightenfactor0

\f2\fs26 \cf0 -- Assets table\
CREATE TABLE assets (\
    asset_id UUID PRIMARY KEY,\
    tenant_id UUID,\
    asset_key TEXT NOT NULL,\
    asset_type TEXT,\
    location TEXT,\
    last_materialized_at TIMESTAMPTZ,\
    last_execution_id UUID\
);\
\
-- Lineage edges\
CREATE TABLE asset_dependencies (\
    downstream_asset_id UUID REFERENCES assets(asset_id),\
    upstream_asset_id UUID REFERENCES assets(asset_id),\
    last_seen_execution_id UUID,\
    PRIMARY KEY (downstream_asset_id, upstream_asset_id)\
);\
\pard\pardeftab720\sa280\partightenfactor0

